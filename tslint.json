{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "interface-name": false,
    "no-console": false,
    "quotemark": [
      true,
      "double",
      "jsx-double"
    ],
    "jsx-alignment": false,
    "jsx-no-multiline-js": false
  }
}