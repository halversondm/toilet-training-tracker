{
  "name": "toilet-training-tracker",
  "version": "1.0.0",
  "description": "An application to track toilet activity for someone in training.",
  "main": "index.js",
  "repository": "https://github.com/halversondm/toilet-training-tracker",
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "prestart": "npm run clean && cp -R app/runtime/* dist && cd dist && npm install",
    "start": "cross-env NODE_ENV=development concurrently --kill-others \"npm run webpack -- --watch\" \"node dist/server.js\"",
    "tslint": "tslint -c tslint.json --fix 'app/**/*.{ts,tsx}'",
    "prewebpack": "npm run tslint",
    "webpack": "webpack --config ./webpack.config.js --profile --colors",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production npm run webpack",
    "tsc": "tsc -w"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "Dan Halverson <daniel.m.halverson@gmail.com> (http://halversondm.com/)",
  "license": "MIT",
  "dependencies": {
    "@types/moment-timezone": "0.2.34",
    "@types/object-assign": "4.0.30",
    "@types/react": "15.0.38",
    "@types/react-dom": "15.5.1",
    "@types/react-redux": "4.4.46",
    "@types/react-router": "4.0.14",
    "@types/requirejs": "2.1.29",
    "aws-sdk": "2.4.13",
    "bootstrap": "3.3.7",
    "exceljs": "0.2.24",
    "font-awesome": "4.7.0",
    "moment": "2.18.1",
    "moment-timezone": "0.5.13",
    "react": "15.6.1",
    "react-bootstrap": "0.31.1",
    "react-data-grid": "2.0.53",
    "react-data-grid-addons": "2.0.54",
    "react-date-picker": "5.3.28",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-router": "4.1.2",
    "react-router-dom": "4.1.2",
    "redux": "3.7.2",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "3.2.1",
    "body-parser": "1.17.2",
    "concurrently": "3.5.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.29.0",
    "json-loader": "0.5.6",
    "morgan": "1.8.2",
    "redux-devtools": "3.4.0",
    "rimraf": "2.6.1",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "tslint": "5.5.0",
    "tslint-react": "3.0.0",
    "typescript": "2.4.2",
    "webpack": "2.4.1"
  }
}
