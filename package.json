    "morgan": "1.7.0",
    "react-transform-catch-errors": "1.0.2",
    "redux-devtools": "3.3.1",
    "rimraf": "2.4.3",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.1",
    "typescript": "2.2.1",
    "webpack": "1.12.14"
  }
