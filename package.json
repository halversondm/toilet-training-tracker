{
  "name": "toilet-training-tracker",
  "version": "1.0.0",
  "description": "An application to track toilet activity for someone in training.",
  "main": "index.js",
  "repository": "https://github.com/halversondm/toilet-training-tracker",
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "prestart": "npm run clean && cp -R app/runtime/* dist && cd dist && npm install",
    "start": "cross-env NODE_ENV=development concurrently --kill-others \"npm run webpack -- --watch\" \"node dist/server.js\"",
    "eslint": "eslint app/**/**.js --fix",
    "prewebpack": "npm run eslint",
    "webpack": "webpack --config ./webpack.config.js --profile --colors",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production npm run webpack",
    "tsc": "tsc -w"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "Dan Halverson <daniel.m.halverson@gmail.com> (http://halversondm.com/)",
  "license": "MIT",
  "dependencies": {
    "@types/moment-timezone": "0.2.34",
    "@types/object-assign": "4.0.30",
    "@types/react": "15.0.12",
    "@types/react-dom": "0.14.23",
    "@types/react-redux": "4.4.36",
    "@types/react-router": "3.0.5",
    "@types/requirejs": "2.1.28",
    "aws-sdk": "2.4.13",
    "bootstrap": "3.3.7",
    "exceljs": "0.2.24",
    "font-awesome": "4.6.3",
    "moment": "2.17.1",
    "moment-timezone": "0.5.11",
    "react": "15.2.1",
    "react-bootstrap": "0.29.5",
    "react-data-grid": "^1.0.42",
    "react-date-picker": "5.3.26",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.5.1",
    "redux": "3.5.2",
    "uuid": "2.0.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "3.0.8",
    "babel-cli": "6.6.5",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-react-display-name": "6.23.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "body-parser": "1.15.2",
    "concurrently": "3.3.0",
    "copy-webpack-plugin": "3.0.1",
